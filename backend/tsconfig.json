{
	"compilerOptions": {
		"target": "ES2022",
		"module": "nodenext",
		"moduleResolution": "nodenext",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "../../.dist/backend",
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": ["./**/*"]
}
